@model Escuela

@{
    ViewData["Title"] = "Info Escuela";
    Layout = "Simple";
}

<h1>Escuela: @ViewData["Title"] </h1>
<h2>Nombre: @Model.Nombre</h2>
<h3>Tipo de Escuel: @Model.TipoEscuela</h3>
<address>
    <p>Direccion: @Model.Dirección</p>
    <p>Pais: @Model.Pais</p>
    <p>Ciudad: @Model.Ciudad</p>
</address>
<p>Año de fundacion: @Model.AñoDeCreacion </p>

<p>@ViewBag.DatoDinamico</p>
